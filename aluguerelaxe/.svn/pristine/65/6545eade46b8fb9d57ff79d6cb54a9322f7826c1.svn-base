package br.com.jcv.aluguerelaxe.client.componente.listbox;

public class SimNaoListBox extends ComponenteListBox {
	
	public SimNaoListBox(){
		this(false,false);
	}
	public SimNaoListBox(boolean b, boolean filtro) {
		super(b);
		this.setWidth("100px");
		if (filtro){
			this.addItem("Indiferente","I");
		}
		this.addItem("Sim","S");                 
		this.addItem("N\u00e3o","N");                 
	}

}
