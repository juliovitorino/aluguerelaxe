<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="br.com.jcv.backend.cidade.CidadeDTO" table="CIDADE">
		<id name="id" column="CIDA_CD_CIDADE">
			<generator class="sequence">
				<param name="sequence">SEQ_CIDA_CD_CIDADE</param>
			</generator>
		</id>
		<property name="nome" column="CIDA_NM_CIDADE" />
	</class>
</hibernate-mapping>