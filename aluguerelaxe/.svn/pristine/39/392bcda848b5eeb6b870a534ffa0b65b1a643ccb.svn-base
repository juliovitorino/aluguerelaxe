package br.com.jcv.aluguerelaxe.client.componente.listbox;


/**
 * <h1>UFListBox</h1>
 * <p>Cria uma ListBox com todos os estados da federação brasileira.</p>
 * @author julio
 *
 */
public class UFListBox extends ComponenteListBox {
	
	public UFListBox(boolean b) {
		super(b);
		this.setWidth("200px");
		this.preencheUF();
	}
	
	public UFListBox() {
		super();
		this.setWidth("200px");
		preencheUF(); 
	}

	private void preencheUF() {
		this.addItem("Selecione um Estado","XX");                 
		this.addItem("Acre","AC");                 
		this.addItem("Alagoas","AL");              
		this.addItem("Amap\u00e1","AP");               
		this.addItem("Amazonas","AM");             
		this.addItem("Bahia","BA");                
		this.addItem("Cear\u00e1","CE");               
		this.addItem("Distrito Federal","DF");     
		this.addItem("Esp\u00edrito Santo","ES");      
		this.addItem("Goi\u00e1s","GO");               
		this.addItem("Maranh\u00e3o","MA");            
		this.addItem("Mato Grosso","MT");          
		this.addItem("Mato Grosso do Sul","MS");   
		this.addItem("Minas Gerais","MG");         
		this.addItem("Paran\u00e1","PR");              
		this.addItem("Para\u00edba","PB");             
		this.addItem("Par\u00e1","PA");                
		this.addItem("Pernambuco","PE");           
		this.addItem("Piau\u00ed","PI");               
		this.addItem("Rio Grande do Norte","RN");  
		this.addItem("Rio Grande do Sul","RS");    
		this.addItem("Rio de Janeiro","RJ");       
		this.addItem("Rond\u00f4nia","RO");            
		this.addItem("Roraima","RR");           
		this.addItem("Santa Catarina","SC");       
		this.addItem("Sergipe","SE");              
		this.addItem("S\u00e3o Paulo","SP");           
		this.addItem("Tocantins","TO");
	}

}
