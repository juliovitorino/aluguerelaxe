<mailing-list>
	<![CDATA[
	/* query para extrair XML Mailing para convidar os indicados para a campanha principal */
	select  '<mailing-record>' ||
			'<nome>' || LEFT(a.assi_nm_assinante, POSITION(' ' IN (a.assi_nm_assinante || ' ')))  || '</nome>' ||
            '<email>' || a.assi_tx_email || '</email>' ||
            '<textolivre1>' || a.assi_tx_campanha || '</textolivre1>' ||
            '<textolivre2></textolivre2>' ||
            '<textolivre3>' || LEFT(a.assi_nm_assinante, POSITION(' ' IN (a.assi_nm_assinante || ' ')))  || '</textolivre3>' ||
            '<textolivre4>'|| (select b.assi_nm_assinante from assinantes b where b.assi_cd_assinante = a.assi_cd_assinante_parent) || '</textolivre4>' ||
            '<textolivre5>'|| (select b.assi_tx_email from assinantes b where b.assi_cd_assinante = a.assi_cd_assinante_parent) || '</textolivre5>' ||
            '<textolivre6></textolivre6>' ||
            '<textolivre7></textolivre7>' ||
            '<textolivre8></textolivre8>' ||
            '<textolivre9></textolivre9>' ||
            '<textolivre10></textolivre10>' ||
			'</mailing-record>' 
    from assinantes a
    where a.assi_cd_assinante_parent > 0
    and   a.assi_in_status = 'A'
    and a.assi_tx_campanha = 'ANIVERSARIO 1 ANO'
    and a.assi_tx_email not in (
                                select c.assi_tx_email
                                from assinantes c
                                where c.assi_cd_assinante_parent = 0
                                and c.assi_tx_campanha = 'ANIVERSARIO 1 ANO'
                            );
	]]>
