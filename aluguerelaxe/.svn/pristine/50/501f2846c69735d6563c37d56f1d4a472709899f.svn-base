package br.com.jcv.aluguerelaxe.client.componente.listbox;


/**
 * <h1>LogradouroListBox</h1>
 * <p>Componente para customização de uma ListBox que apresenta os valores padrões
 * de logradouro</p>
 * @author julio
 *
 */
public class FormaPagamentoReservaListBox extends ComponenteListBox {
	
	public FormaPagamentoReservaListBox() {
		super();
		this.addItem("-- Escolha a forma de pagamento --", "X");
		this.addItem("50% Entrada + 50% na Entrega da Chave", "P");
		this.addItem("100% Entrada", "T");
	}
	

}
