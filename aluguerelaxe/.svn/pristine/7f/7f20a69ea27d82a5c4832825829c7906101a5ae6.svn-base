package br.com.jcv.aluguerelaxe.client.administracao.console.tarefaspendentes;

import com.google.gwt.user.client.ui.AbstractImagePrototype;
import com.google.gwt.user.client.ui.ImageBundle.Resource;
import com.google.gwt.user.client.ui.TreeImages;

/**
* Interface para pacote de imagens que compoe a treeview de tarefas pendentes
* @author Julio Vitorino
*/
public interface TarefasPendentesTreeImages extends TreeImages {
    
    @Resource("downArrow.png")
    AbstractImagePrototype treeOpen();
    
    @Resource("rightArrow.png")
    AbstractImagePrototype treeClosed();

    @Resource("folha.png")
    AbstractImagePrototype treeLeaf();
}
  
