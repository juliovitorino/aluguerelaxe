package br.com.jcv.backend.utility;

public class FirebirdRecord {

	private int gdscode;
	private String symbol;
	private String textoOriginal;
	
	public FirebirdRecord(int gdscode, String symbol, String textoOriginal) {
		this.gdscode = gdscode;
		this.symbol = symbol;
		this.textoOriginal = textoOriginal;
	}

	/**
	 * Este método é responsável por retornar o
	 * conteúdo de 'gdscode'.
	 * @return Retorna um objeto do tipo int
	 */
	public int getGdscode() {
		return this.gdscode;
	}

	/**
	 * Este método é responsável por retornar o
	 * conteúdo de 'symbol'.
	 * @return Retorna um objeto do tipo String
	 */
	public String getSymbol() {
		return this.symbol;
	}

	/**
	 * Este método é responsável por retornar o
	 * conteúdo de 'textoOriginal'.
	 * @return Retorna um objeto do tipo String
	 */
	public String getTextoOriginal() {
		return this.textoOriginal;
	}
	
}

