package br.com.jcv.aluguerelaxe.client.interfaces;

/**
 * <h1>Navegador</h1>
 * <p>Definição de métodos que devem ser implementados para classes que devem usar navegadores
 * </p>
 * 
 * @author Julio
 * @since 1.0
 */
public interface Navegador {

     /**
      * <h2>irParaPrimeiro</h2>
      * <p>definição do método a ser implementado para navegar diretamente para o primeiro item</p>
      */
     void irParaPrimeiro();
     /**
      * <h2>irParaUltimo</h2>
      * <p>definição do método a ser implementado para navegar diretamente para o último item</p>
      */
     void irParaUltimo();
     /**
      * <h2>irParaProximo</h2>
      * <p>definição do método a ser implementado para navegar para o próximo item</p>
      */
     void irParaProximo();
     /**
      * <h2>irParaAnterior</h2>
      * <p>definição do método a ser implementado para navegar para o item anterior</p>
      */
     void irParaAnterior();
}
