package br.com.jcv.aluguerelaxe.client.componente.datagrid;

/**
* <h2>DefinicaoCampoDataGrid</h2>
* <p>Definicao do campo e suas caracteristicas para compor DefinicaoDataGrid
* </p>
* @author Julio
*/
public class DefinicaoCampoDataGrid {
	private int sequencia;
	private String header;
	private String field;
	private boolean ordem;
	
	public DefinicaoCampoDataGrid(int sequencia, String header, String field) {
		this.sequencia = sequencia;
		this.header = header;
		this.field = field;
		this.ordem = false;
	}
	
	public String getHeader() {
		return this.header;
	}
	
	public String getField() {
		return this.field;
	}
	
}