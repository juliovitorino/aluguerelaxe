<spammer-config>
	<!-- Descreva a tarefa deste config -->
	<tarefa>Mailing Full =></tarefa>

	<!-- Descreva aqui o subject do e-mail -->
	<assunto>${TSPI_TAG_IFC_NOME_1}, já pensou em imóvel de temporada?</assunto>
	
	<!-- Template html para ser enviado durante o processo -->
	<template>/home/aluguerelaxe/spammer/templates/template-novo-newsletter.html</template>

	<!-- lista xml contendo os e-mails para contatos -->
	<catalogo>/home/aluguerelaxe/spammer/mailing/mailing-full.xml</catalogo>
	
	<!-- lista de publicadores da mensagem -->
	<publicador nome="AlugueRelaxe News" email="info@aluguerelaxe.com.br"/>
	
	<!-- tempo de espera entre envio de cada e-mail unidade em milisegundos -->
	<tempoespera>0</tempoespera>

	<!-- Identifica o modo de envio QUEUE ou EMAIL (default) -->
	<modoenvio>QUEUE</modoenvio>
	
	<!-- Path de enfileiramento -->
	<queuePath>/var/aluguerelaxe/data/emails/queue</queuePath>

	
	<!-- ativa modo de leitura da lista de emails do final para o inicio -->
	<modoreverso>ON</modoreverso>

	<!-- Lista de smtp mail servers local ou na VM a serem utilizados no processo -->
	<servidorEmail nome="mail.aluguerelaxe.com.br" porta="25" contaAutenticacao="info@aluguerelaxe.com.br" aliaspwd="JUPITER_SMTP_PWD"/>

