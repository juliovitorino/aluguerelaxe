package br.com.jcv.aluguerelaxe.client.componente.field;

import com.google.gwt.user.client.ui.TextBox;


/**
 * <h2>FieldCompositeTextBox</h2>
 * <p>Classe responsavel por montar um composite composto dentro de um VerticalPanel
 * na referida sequencia:<br>
 * <li>Label</li>
 * <li>TextBox</li>
 * </p>
 * 
 * @author Julio
 *
 */
public class FieldCompositeTextBox extends AbstractFieldComposite<TextBox> {

	public FieldCompositeTextBox(String tituloLabel) {
		super(tituloLabel, new TextBox());
	}

}
